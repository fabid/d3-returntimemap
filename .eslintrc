{
    "globals": {
      "window": true,
      "d3": true,
    },
    "rules": {
        "indent": [2, 2, {"VariableDeclarator": 2}],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "comma-dangle": [2, "always-multiline"],
        "semi": [2, "always"]
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended"
}
